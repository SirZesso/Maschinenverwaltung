<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
    <children>
      <VBox prefHeight="200.0" prefWidth="350.0">
         <children>
            <StackPane prefHeight="109.0" prefWidth="350.0">
               <children>
                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Login Maschinenverwaltung [DEMO]" textAlignment="CENTER" textOrigin="CENTER" wrappingWidth="239.0">
                          <font>
                              <Font name="Avenir Next Bold" size="15.0" />
                          </font>
                      </Text>
               </children>
            </StackPane>
              <HBox alignment="TOP_CENTER" prefHeight="20.0" prefWidth="350.0" spacing="10.0">
                  <children>
                      <Button contentDisplay="CENTER" onAction="#btnClickSupplier" prefHeight="25.0" prefWidth="90.0" text="Hersteller" />
                      <Button contentDisplay="CENTER" defaultButton="true" onAction="#btnClickCustomer" prefHeight="25.0" prefWidth="90.0" text="Kunde" />
                  </children>
                  <padding>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
              </HBox>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="350.0">
               <children>
                  <Button blendMode="MULTIPLY" mnemonicParsing="false" onAction="#createDummyData" prefHeight="25.0" prefWidth="191.0" text="Dummy-Daten generieren" />
               </children>
            </HBox>
         </children>
      </VBox>
    </children>
</AnchorPane>
